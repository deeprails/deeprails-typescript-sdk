{
  "dxt_version": "0.2",
  "name": "deeprails-mcp",
  "version": "1.0.0",
  "description": "The official MCP Server for the Deeprails API",
  "author": {
    "name": "Deeprails",
    "email": "support@deeprails.ai"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deeprails/deeprails-typescript-sdk.git"
  },
  "homepage": "https://github.com/deeprails/deeprails-typescript-sdk/tree/main/packages/mcp-server#readme",
  "documentation": "https://docs.deeprails.com",
  "server": {
    "type": "node",
    "entry_point": "index.js",
    "mcp_config": {
      "command": "node",
      "args": ["${__dirname}/index.js"],
      "env": {
        "DEEPRAILS_API_KEY": "${user_config.DEEPRAILS_API_KEY}"
      }
    }
  },
  "user_config": {
    "DEEPRAILS_API_KEY": {
      "title": "api_key",
      "description": "",
      "required": true,
      "type": "string"
    }
  },
  "tools": [],
  "tools_generated": true,
  "compatibility": {
    "runtimes": {
      "node": ">=18.0.0"
    }
  },
  "keywords": ["api"]
}
